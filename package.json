{
  "name": "gitify",
  "version": "4.4.0",
  "description": "GitHub Notifications on your menu bar.",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.common.js --watch",
    "make:linux": "electron-builder --linux",
    "make:macos": "electron-builder --mac --universal",
    "make:win": "electron-builder --win",
    "prettier:check": "prettier --check 'src/**/*.{js,ts,tsx}'",
    "prettier:apply": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "test": "jest",
    "start": "electron . --enable-logging"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gitify-app/gitify.git"
  },
  "keywords": [
    "gitify",
    "github",
    "notifier",
    "notifications",
    "electron",
    "atom",
    "shell",
    "app",
    "tray"
  ],
  "author": {
    "name": "Emmanouil Konstantinidis",
    "email": "hello@manos.im"
  },
  "contributors": [
    {
      "name": "Emmanouil Konstantinidis",
      "url": "https://github.com/manosim"
    },
    {
      "name": "Jake 'Sid' Smith",
      "url": "https://github.com/JakeSidSmith"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manosim/gitify/issues"
  },
  "homepage": "https://www.gitify.io/",
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "setupFiles": [
      "<rootDir>/src/__helpers__/setupEnvVars.js"
    ],
    "testEnvironment": "jsdom",
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  },
  "build": {
    "appId": "com.electron.gitify",
    "productName": "Gitify",
    "files": [
      "build/**/*",
      "assets/**/*",
      "index.html",
      "LICENSE",
      "main.js",
      "first-run.js"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/images/app-icon.icns",
      "identity": "Emmanouil Konstantinidis (3YP8SXP3BF)",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./entitlements/entitlements.mac.plist",
      "entitlementsInherit": "./entitlements/entitlements.mac.plist",
      "publish": [
        "github"
      ]
    },
    "dmg": {
      "icon": "assets/images/app-icon.icns",
      "sign": false
    },
    "win": {
      "target": "nsis",
      "icon": "assets/images/app-icon.ico"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development",
      "maintainer": "Emmanouil Konstantinidis"
    },
    "afterSign": "scripts/notarize.js"
  },
  "dependencies": {
    "@primer/octicons-react": "19.8.0",
    "axios": "0.27.2",
    "date-fns": "2.16.1",
    "electron-updater": "6.1.4",
    "final-form": "4.20.10",
    "history": "4.10.1",
    "lodash": "4.17.21",
    "menubar": "9.3.0",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-emojione": "5.0.1",
    "react-final-form": "6.4.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-transition-group": "4.4.1",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "29.5.5",
    "@types/lodash": "4.14.199",
    "@types/node": "18.18.0",
    "@types/react": "18.2.24",
    "@types/react-router-dom": "5.3.3",
    "@types/react-transition-group": "4.4.7",
    "autoprefixer": "10.4.16",
    "coveralls": "3.1.1",
    "css-loader": "6.8.1",
    "electron": "^13.1.7",
    "electron-builder": "24.6.4",
    "electron-notarize": "1.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "nock": "13.3.3",
    "postcss": "8.4.30",
    "postcss-loader": "7.3.3",
    "prettier": "3.0.3",
    "react-test-renderer": "18.2.0",
    "style-loader": "3.3.3",
    "tailwindcss": "3.3.3",
    "ts-jest": "29.1.1",
    "webpack": "5.83.1",
    "webpack-cli": "5.1.1",
    "webpack-merge": "5.8.0"
  },
  "packageManager": "pnpm@8.7.6"
}
