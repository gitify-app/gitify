{
  env: {
    browser: true,
    es6: true,
    node: true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
      "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": [2, "global"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "comma-style": [2, "last"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "no-multiple-empty-lines": [2, {"max": 2}],
    "max-len": [2, 120, 2, {"ignoreComments": true}],
    "no-trailing-spaces": [2, {"skipBlankLines": false}],
    "dot-location": [2, "property"],
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": 0,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "no-lone-blocks": 2,
    "eqeqeq": 2,
    "block-scoped-var": 2,
    "no-constant-condition": 2,
    "no-nested-ternary": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-undef": 2,
    "no-unreachable": 2,
    "no-dupe-keys": 2,
    "no-extra-boolean-cast": 2,
    "no-irregular-whitespace": 2,
    "no-multi-spaces": 2,
    "no-else-return": 0,
    "no-eval": 2,
    "no-multi-str": 2,
    "no-self-compare": 2,
    "no-useless-call": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-use-before-define": 2,
    "radix": 2,
    "curly": 2,
    "no-fallthrough": 0,
    "default-case": 0,
    "no-plusplus": 0,
    "dot-notation": 0,
    "jsx-quotes": [2, "prefer-double"],
    "eol-last": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],

    "react/jsx-uses-vars": 2,
  }
}
